   "source": [
    "import subprocess\n",
    "import os\n",
    "from PIL import Image\n",
    "from tqdm import tqdm\n",
    "\n",
    "\n",
    "# å…ˆå°†å›¾ç‰‡è‡ªåŠ¨å¤„ç†ä¸ºRGBæ¨¡å¼ï¼ˆé»˜è®¤ä¸ºRGBAï¼‰\n",
    "# 1. å®šä¹‰è·¯å¾„\n",
    "input_dir = \"/kaggle/input/ruishou/ruishou\"  # åŸå§‹å›¾åƒç›®å½•\n",
    "temp_rgb_dir = \"/kaggle/working/temp_rgb\"  # ä¸´æ—¶å­˜æ”¾RGBè½¬æ¢åçš„å›¾åƒ\n",
    "output_zip = \"/kaggle/working/my_dataset-512x512.zip\"  # æœ€ç»ˆè¾“å‡ºçš„ZIPæ–‡ä»¶\n",
    "\n",
    "# 2. åˆ›å»ºä¸´æ—¶ç›®å½•\n",
    "os.makedirs(temp_rgb_dir, exist_ok=True)\n",
    "\n",
    "# 3. æ‰¹é‡è½¬æ¢å›¾åƒä¸ºRGBæ ¼å¼\n",
    "image_files = [f for f in os.listdir(input_dir) if f.lower().endswith(('.png', '.jpg', '.jpeg'))]\n",
    "\n",
    "for filename in tqdm(image_files, desc=\"è½¬æ¢å›¾åƒæ ¼å¼\"):\n",
    "    try:\n",
    "        # æ‰“å¼€å›¾åƒ\n",
    "        img_path = os.path.join(input_dir, filename)\n",
    "        with Image.open(img_path) as img:\n",
    "            # è½¬æ¢ä¸ºRGBï¼ˆè‡ªåŠ¨å¤„ç†RGBAã€Pæ¨¡å¼ç­‰ï¼‰\n",
    "            if img.mode not in ('RGB', 'L'):  # 'L'æ˜¯ç°åº¦æ¨¡å¼\n",
    "                img = img.convert('RGB')\n",
    "            # ä¿å­˜åˆ°ä¸´æ—¶ç›®å½•\n",
    "            save_path = os.path.join(temp_rgb_dir, filename)\n",
    "            img.save(save_path)\n",
    "    except Exception as e:\n",
    "        print(f\"å¤„ç† {filename} å¤±è´¥: {e}\")\n",
    "\n",
    "\n",
    "\n",
    "# 4. ä½¿ç”¨è½¬æ¢åçš„RGBå›¾åƒè¿›è¡Œæ‰“åŒ…\n",
    "command = [\n",
    "    \"python\", \"dataset_tool.py\",\n",
    "    \"--source\", temp_rgb_dir,\n",
    "    \"--dest\", output_zip,\n",
    "    \"--resolution\", \"512x512\"\n",
    "]\n",
    "\n",
    "result = subprocess.run(\n",
    "    command,\n",
    "    capture_output=True,\n",
    "    text=True\n",
    ")\n",
    "\n",
    "# æ‰“å°ç»“æœ\n",
    "print(\"\\næ‰“åŒ…å‘½ä»¤è¾“å‡ºï¼š\")\n",
    "print(result.stdout)\n",
    "print(\"é”™è¯¯ä¿¡æ¯ï¼š\")\n",
    "print(result.stderr)\n",
    "\n",
    "if result.returncode == 0:\n",
    "    print(f\"\\næ•°æ®é›†æ‰“åŒ…æˆåŠŸï¼Œä¿å­˜è‡³ï¼š{output_zip}\")\n",
    "else:\n",
    "    print(\"\\næ•°æ®é›†æ‰“åŒ…å¤±è´¥\")\n"
   ]
